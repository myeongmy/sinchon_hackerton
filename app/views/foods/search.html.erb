
<table>
  <thead>
    <tr>
      <th>Fname</th>
      <th>Material1</th>
      <th>Material2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foods.each do |food| %>
      <tr>
        <td><%= food.fname %></td>
        <td><%= food.material1 %></td>
        <td><%= food.material2 %></td>
        <td><%= link_to 'Show', food %></td>
        <td><%= link_to 'Edit', edit_food_path(food) %></td>
        <td><%= link_to 'Destroy', food, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
